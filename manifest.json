{
    "name": "Glove",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Helping you hide your fingerprint - by Net-Comet PTE LTD",
    "homepage_url": "http://net-comet.com/glove",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "browser action demo",
        "default_popup": "src/browser_action/browser_action.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "js/dostuff.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/inject/inject.js"
            ],
            "run_at": "document_start"
        }
    ]
}